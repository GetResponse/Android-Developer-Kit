<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.getresponse.sampleapp"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".contacts.CreateContactActivity"
            android:label="@string/add_simple_contact"/>

        <activity
            android:name=".contacts.GetContactsActivity"
            android:label="@string/title_activity_get_contacts"/>

        <activity
            android:name=".contacts.DeleteContactActivity"
            android:label="@string/delete_contact"/>
    </application>

</manifest>
